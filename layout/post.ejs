<article class="markdown md:w-3/5">
  <div>
    <h1>
      <%= page.title %>
    </h1>
  </div>
  <div class="my-2 text-overlay1 text-sm">
    <%- remapDate(date(page.date)) %>
    <span>,</span>
    <span><%- date(page.date, "YYYY" ) %></span>
  </div>
  <%- toc(page.content, {list_number: false}) %>
    <div>
      <%- page.content %>
    </div>
</article>
